// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model CustomerGroup {
  code        String   @id @map("_id") @db.String
  name        String
  description String
  createDate  DateTime @default(now())

  factories Factory[]
}

type CustomerPic {
  name        String
  email       String
  phoneNumber String
  telephone   String
  fax         String
}

model Factory {
  code       String        @id @map("_id") @db.String
  name       String
  group      CustomerGroup @relation(fields: [groupCode], references: [code], onDelete: Cascade)
  groupCode  String        @db.String
  address    String
  province   String
  city       String
  telephone  String
  fax        String
  email      String
  top        Int
  currency   String
  purchasing CustomerPic
  operation  CustomerPic
  finance    CustomerPic
  createDate DateTime      @default(now())
  status     Boolean       @default(false)
}

model Vendor {
  code         String        @id @map("_id") @db.String
  name         String
  address      String
  province     String
  city         String
  telephone    String
  fax          String
  email        String
  top          Int
  currency     String
  purchasing   CustomerPic
  operation    CustomerPic
  finance      CustomerPic
  createDate   DateTime      @default(now())
  status       Boolean       @default(false)
  priceVendors PriceVendor[]
  vehicles     Vehicle[]
  uangJalan    UangJalan[]
}

model Shipping {
  code          String          @id @map("_id") @db.String
  name          String
  address       String
  province      String
  city          String
  telephone     String
  fax           String
  email         String
  top           Int
  currency      String
  purchasing    CustomerPic
  operation     CustomerPic
  finance       CustomerPic
  createDate    DateTime        @default(now())
  status        Boolean         @default(false)
  PriceShipping PriceShipping[]
  Vessel        Vessel[]
}

model Route {
  code                 String                @id @map("_id") @db.String
  containerSize        String
  containerType        String
  serviceType          String
  province             String
  city                 String
  startDescription     String
  endDescription       String
  effectiveStartDate   DateTime
  effectiveEndDate     DateTime
  createDate           DateTime              @default(now())
  priceVendorDetails   PriceVendorDetail[]
  priceShippingDetails PriceShippingDetail[]
  UangJalan            UangJalan[]
}

model Port {
  code                 String                @id @map("_id") @db.String
  province             String
  city                 String
  name                 String
  createDate           DateTime              @default(now())
  priceVendorDetails   PriceVendorDetail[]
  priceShippingDetails PriceShippingDetail[]
}

model PriceVendorDetail {
  id            String      @id @default(auto()) @map("_id") @db.ObjectId
  priceVendor   PriceVendor @relation(fields: [priceVendorId], references: [id], onDelete: Cascade)
  priceVendorId String      @db.ObjectId
  route         Route       @relation(fields: [routeCode], references: [code], onDelete: Cascade)
  routeCode     String      @db.String
  containerSize String
  containerType String
  serviceType   String
  port          Port        @relation(fields: [portCode], references: [code], onDelete: Cascade)
  portCode      String      @db.String
  tracking      Int
  buruh         Int
  thcOPT        Int
  thcOPP        Int
  adminBL       Int
  cleaning      Int
  materai       Int
}

model PriceVendor {
  id                 String              @id @default(auto()) @map("_id") @db.ObjectId
  vendor             Vendor              @relation(fields: [vendorCode], references: [code], onDelete: Cascade)
  vendorCode         String              @db.String
  containerSize      String
  containerType      String
  serviceType        String
  effectiveStartDate DateTime
  effectiveEndDate   DateTime
  details            PriceVendorDetail[]
  createDate         DateTime            @default(now())
  status             Boolean
}

model PriceShippingDetail {
  id              String        @id @default(auto()) @map("_id") @db.ObjectId
  priceShipping   PriceShipping @relation(fields: [priceShippingId], references: [id], onDelete: Cascade)
  priceShippingId String        @db.ObjectId
  route           Route         @relation(fields: [routeCode], references: [code], onDelete: Cascade)
  routeCode       String        @db.String
  containerSize   String
  containerType   String
  serviceType     String
  port            Port          @relation(fields: [portCode], references: [code], onDelete: Cascade)
  portCode        String        @db.String
  freight         Int
  thcOPT          Int
  thcOPP          Int
  adminBL         Int
  cleaning        Int
  alihKapal       Int
  materai         Int
  lolo            Int
  segel           Int
  rc              Int
  lss             Int
}

model PriceShipping {
  id                  String                @id @default(auto()) @map("_id") @db.ObjectId
  shipping            Shipping              @relation(fields: [shippingCode], references: [code], onDelete: Cascade)
  shippingCode        String                @db.String
  containerSize       String
  containerType       String
  serviceType         String
  effectiveStartDate  DateTime
  effectiveEndDate    DateTime
  PriceShippingDetail PriceShippingDetail[]
}

model ProductCategory {
  reff       String    @id @map("_id") @db.String
  name       String
  createDate DateTime  @default(now())
  products   Product[]
}

enum ProductType {
  Product
  Sparepart
  ATK
}

model Product {
  skuCode             String           @id @map("_id") @db.String
  type                ProductType
  productCategory     ProductCategory? @relation(fields: [productCategoryReff], references: [reff], onDelete: Cascade)
  productCategoryReff String?          @db.String
  name                String
  unit                String
  createDate          DateTime         @default(now())
}

model Sales {
  code        String           @id @map("_id") @db.String
  jobPosition SalesJobPosition
  name        String
  nik         String
  area        String
  phoneNumber String
  telephone   String
  fax         String
  email       String
  status      Boolean          @default(false)
  createDate  DateTime         @default(now())
}

enum SalesJobPosition {
  Marketing
  Direktur
}

model Vehicle {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  vendor        Vendor   @relation(fields: [vendorCode], references: [code])
  vendorCode    String   @db.String
  truckNumber   String
  brand         String
  type          String
  machineNumber String
  frameNumber   String
  cylinder      String
  color         String
  stnkExpired   DateTime
  taxExpired    DateTime
  keurExpired   DateTime
  createDate    DateTime @default(now())
  status        Boolean
}

model Vessel {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  shipping     Shipping @relation(fields: [shippingCode], references: [code])
  shippingCode String   @db.String
  name         String
  unit         String
  capacity     Int
  createDate   DateTime @default(now())
  status       Boolean
}

model UangJalan {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  vendor        Vendor   @relation(fields: [vendorCode], references: [code])
  vendorCode    String   @db.String
  route         Route    @relation(fields: [routeCode], references: [code])
  routeCode     String   @db.String
  truckType     String
  containerSize String
  bbm           Int
  toll          Int
  labourCost    Int
  meal          Int
  etc           Int
  total         Int
  createDate    DateTime @default(now())
}

model Coa {
  number      String   @id @map("_id") @db.String
  description String
  type        String
  category    String
  transaction String
  currency    String
  status      Boolean
  createDate  DateTime @default(now())

  coas1 Coa1[]
}

model Coa1 {
  number      String @id @map("_id") @db.String
  coa         Coa    @relation(fields: [coaNumber], references: [number])
  coaNumber   String @db.String
  description String

  coas2 Coa2[]
}

model Coa2 {
  number      String @id @map("_id") @db.String
  coa1        Coa1   @relation(fields: [coa1Number], references: [number])
  coa1Number  String @db.String
  description String
}
