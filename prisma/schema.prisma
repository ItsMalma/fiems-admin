// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model CustomerGroup {
  code        String    @id @map("_id") @db.String
  name        String
  description String
  createDate  DateTime  @default(now())
  factories   Factory[]
}

type CustomerPic {
  name        String
  email       String
  phoneNumber String
  telephone   String
  fax         String
}

model Factory {
  code       String        @id @map("_id") @db.String
  name       String
  group      CustomerGroup @relation(fields: [groupCode], references: [code])
  groupCode  String        @db.String
  address    String
  province   String
  city       String
  telephone  String
  fax        String
  email      String
  top        Int
  currency   String
  purchasing CustomerPic
  operation  CustomerPic
  finance    CustomerPic
  createDate DateTime      @default(now())
  status     Boolean       @default(false)
}

model Vendor {
  code         String        @id @map("_id") @db.String
  name         String
  address      String
  province     String
  city         String
  telephone    String
  fax          String
  email        String
  top          Int
  currency     String
  purchasing   CustomerPic
  operation    CustomerPic
  finance      CustomerPic
  createDate   DateTime      @default(now())
  status       Boolean       @default(false)
  priceVendors PriceVendor[]
}

model Shipping {
  code           String          @id @map("_id") @db.String
  name           String
  address        String
  province       String
  city           String
  telephone      String
  fax            String
  email          String
  top            Int
  currency       String
  purchasing     CustomerPic
  operation      CustomerPic
  finance        CustomerPic
  createDate     DateTime        @default(now())
  status         Boolean         @default(false)
  priceShippings PriceShipping[]
}

model Route {
  code                 String                @id @map("_id") @db.String
  serviceType          String
  containerType        String
  province             String
  city                 String
  startDescription     String
  endDescription       String
  effectiveStartDate   DateTime
  effectiveEndDate     DateTime
  createDate           DateTime              @default(now())
  priceVendorDetails   PriceVendorDetail[]
  priceShippingDetails PriceShippingDetail[]
}

model Port {
  code                 String                @id @map("_id") @db.String
  province             String
  city                 String
  name                 String
  createDate           DateTime              @default(now())
  priceVendorDetails   PriceVendorDetail[]
  priceShippingDetails PriceShippingDetail[]
}

model PriceVendorDetail {
  id            String      @id @map("_id") @db.ObjectId
  priceVendor   PriceVendor @relation(fields: [priceVendorId], references: [id])
  priceVendorId String      @db.ObjectId
  route         Route       @relation(fields: [routeCode], references: [code])
  routeCode     String      @db.String
  containerSize String
  containerType String
  serviceType   String
  port          Port        @relation(fields: [portCode], references: [code])
  portCode      String      @db.String
  trackingRate  Int
  buruh         Int
  thcOPT        Int
  thcOPP        Int
  adminBL       Int
  cleaning      Int
  materai       Int
}

model PriceVendor {
  id                 String              @id @map("_id") @db.ObjectId
  vendor             Vendor              @relation(fields: [vendorCode], references: [code])
  vendorCode         String              @db.String
  containerSize      String
  containerType      String
  serviceType        String
  effectiveStartDate DateTime
  effectiveEndDate   DateTime
  details            PriceVendorDetail[]
  createDate         DateTime            @default(now())
  status             Boolean
}

model PriceShippingDetail {
  id              String        @id @map("_id") @db.ObjectId
  priceShipping   PriceShipping @relation(fields: [priceShippingId], references: [id])
  priceShippingId String        @db.ObjectId
  route           Route         @relation(fields: [routeCode], references: [code])
  routeCode       String        @db.String
  containerSize   String
  containerType   String
  serviceType     String
  port            Port          @relation(fields: [portCode], references: [code])
  portCode        String        @db.String
  freight         Int
  thcOPT          Int
  thcOPP          Int
  adminBL         Int
  cleaning        Int
  alihKapal       Int
  materai         Int
  lolo            Int
  segel           Int
  rc              Int
  lss             Int
}

model PriceShipping {
  id                 String                @id @map("_id") @db.ObjectId
  shipping           Shipping              @relation(fields: [shippingCode], references: [code])
  shippingCode       String                @db.String
  containerSize      String
  containerType      String
  serviceType        String
  effectiveStartDate DateTime
  effectiveEndDate   DateTime
  details            PriceShippingDetail[]
  createDate         DateTime              @default(now())
  status             Boolean
}

model ProductCategory {
  reff       String    @id @map("_id") @db.String
  name       String
  createDate DateTime  @default(now())
  products   Product[]
}

enum ProductType {
  Product
  Sparepart
  ATK
}

model Product {
  skuCode             String           @id @map("_id") @db.String
  type                ProductType
  productCategory     ProductCategory? @relation(fields: [productCategoryReff], references: [reff])
  productCategoryReff String?          @db.String
  name                String
  unit                String
  createDate          DateTime         @default(now())
}
